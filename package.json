{
  "name": "ae2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ampersand-app": "2.0.0",
    "history": "4.6.3",
    "lodash": "4.17.4",
    "material-ui": "0.18.3",
    "mobx": "3.1.16",
    "mobx-react": "4.2.1",
    "react": "15.6.1",
    "react-autosuggest": "9.3.0",
    "react-contextmenu": "2.6.3",
    "react-dom": "15.6.1",
    "react-linkify": "0.2.1",
    "react-reflex": "2.0.3",
    "react-relay": "1.0.0",
    "react-tap-event-plugin": "2.0.1",
    "react-virtualized": "9.8.0",
    "recompose": "0.23.5",
    "styled-components": "2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.0",
    "babel-plugin-relay": "1.0.1",
    "babel-preset-react-app": "3.0.0",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "5.1.0",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.0.0",
    "eslint-config-react-app": "1.0.4",
    "eslint-loader": "1.8.0",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.5.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "flow-bin": "0.48.0",
    "fs-extra": "3.0.1",
    "hpack.js": "^2.1.6",
    "html-webpack-plugin": "2.28.0",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "obuf": "^1.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.0",
    "react-dev-utils": "3.0.0",
    "react-error-overlay": "1.0.7",
    "relay-compiler": "1.0.0",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "wbuf": "^1.7.2",
    "webpack": "3.0.0",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "npm run build-relay && node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "flow": "flow",
    "schema": "node scripts/schema.js",
    "build-relay": "relay-compiler --src ./src --schema ./schema/schema.graphql",
    "watch-relay": "npm run build-relay -- --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      ".+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      ".+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "relay"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "graphql": {
    "request": {
      "url": "http://localhost:5000/graphql"
    }
  }
}
